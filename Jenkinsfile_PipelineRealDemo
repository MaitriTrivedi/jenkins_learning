pipeline { 
    agent any
    stages {
        stage('Clone Git') {
            steps {
                git 'https://github.com/MaitriTrivedi/jenkins_learning.git'
            }
        }
        stage('Build Code') {
            steps {
                sh "chmod u+x PipelineRealDemo.py"
                sh "./PipelineRealDemo.py"
            }
        }
         stage('Test Code') {
            steps {
                sh "chmod u+x TestPipelineRealDemo.py"
                sh "./TestPipelineRealDemo.py"
            }
        }
    } 
}
